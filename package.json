{
  "name": "enterprise-scaffold",
  "version": "1.0.0",
  "description": "企业级Node.js项目脚手架",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js,json,md}\"",
    "seed": "node src/utils/simpleSeed.js",
    "db:stats": "node scripts/db-manager.js",
    "db:migrate": "node scripts/migrate.js",
    "db:backup": "node scripts/backup.js",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t enterprise-scaffold .",
    "docker:run": "docker run -p 3000:3000 enterprise-scaffold",
    "docs:generate": "swagger-jsdoc -d swaggerDef.js src/routes/*.js -o docs/swagger.json",
    "docs:serve": "swagger-ui-serve docs/swagger.json",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit"
  },
  "keywords": [
    "nodejs",
    "express",
    "enterprise",
    "scaffold",
    "api",
    "mongodb",
    "jwt",
    "swagger"
  ],
  "author": "Enterprise Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.2",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-slow-down": "^2.0.0",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "redis": "^4.6.13",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.2",
    "nodemailer": "^6.9.8",
    "twilio": "^4.20.1",
    "stripe": "^14.10.0",
    "aws-sdk": "^2.1550.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "socket.io": "^4.7.4",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
    "cron": "^3.1.6",
    "node-cron": "^3.0.3",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "puppeteer": "^21.6.1",
    "csv-parser": "^3.0.0",
    "exceljs": "^4.4.0",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.3",
    "bcrypt": "^5.1.1",
    "crypto-js": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.1.0",
    "connect-redis": "^7.1.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "faker": "^6.6.6",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^22.0.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
